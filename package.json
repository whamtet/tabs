{
  "name": "tabs",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "shadow-cljs": "2.28.12",
    "tailwindcss": "^3.4.10"
  },
  "scripts": {
    "tailwind": "tailwindcss -i ./src/input.css -o ./public/output.css --watch",
